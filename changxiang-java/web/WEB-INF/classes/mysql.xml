<?xml version="1.0" encoding="UTF-8"?>
<proxool-config>
	<proxool>
		<alias>dbname</alias>
		<driver-url>jdbc:mysql://127.0.0.1:3306/test?autoReconnect=true&amp;useUnicode=true&amp;characterEncoding=utf8&amp;mysqlEncoding=utf8</driver-url>
		<driver-class>com.mysql.jdbc.Driver</driver-class>
		<driver-properties>
			<property name="user" value="root" />
			<property name="password" value="123456" />
		</driver-properties>
		<house-keeping-sleep-time>20000</house-keeping-sleep-time>
        <simultaneous-build-throttle>20</simultaneous-build-throttle>
        <prototype-count>5</prototype-count>    
        <maximum-connection-count>450</maximum-connection-count> 
        <minimum-connection-count>30</minimum-connection-count>
    	<house-keeping-test-sql>select CURRENT_DATE</house-keeping-test-sql>   
	</proxool>
</proxool-config>